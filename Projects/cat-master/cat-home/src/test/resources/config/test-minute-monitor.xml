<monitor-rules>
	<rule id="two-minute">
		<config starttime="00:00" endtime="24:00">
			<condition minute="2">
					<sub-condition type="DescPer" text="40"></sub-condition>
			</condition>
			<condition minute="2">
					<sub-condition type="DescVal" text="80"></sub-condition>
			</condition>
		</config>
	</rule>
</monitor-rules>
